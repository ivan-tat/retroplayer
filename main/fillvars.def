_DATA segment word public use16 'DATA'

; DMA buffer

extrn AllocBuffer: dword
extrn DMAbuffer: dword
extrn PlayBuffer: dword
extrn NumBuffers: byte
extrn LastReady: byte
extrn DMAHalf: byte
extrn DMARealBufSize: word

; mixer

extrn TickBuffer: dword
extrn LQMode: byte
extrn TooSlow: byte
extrn JustInFill: byte
extrn RasterTime: byte
extrn FPS: byte

; EMM

extrn savHandle: word

_DATA ends
