_DATA segment word public use16 'DATA'

; DMA buffer

extrn DMABufUnaligned: dword
extrn DMABuf: dword
extrn DMABufSize: dword
extrn DMABufFrameSize: word
extrn DMABufFramesCount: byte
extrn DMABufFrameLast: byte
extrn DMABufFrameActive: byte
extrn DMAFlags_JustInFill: byte
extrn DMAFlags_Slow: byte

; player

extrn playOption_FPS: byte
extrn playOption_LowQuality: byte

; EMM

extrn savHandle: word

_DATA ends
